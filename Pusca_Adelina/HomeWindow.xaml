<Window x:Class="Pusca_Adelina.HomeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pusca_Adelina"
        mc:Ignorable="d"
        Title="Home" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="images/home.jpg"/>
        </Grid.Background>
        <Button x:Name="adminBtn" Content="Admin" HorizontalAlignment="Left" Height="49" Margin="233,234,0,0" VerticalAlignment="Top" Width="177" Background="#FFD173C0" Foreground="White" FontWeight="Bold" FontSize="16" Click="adminBtn_Click" BorderBrush="#FFD173C0"/>
        <Button x:Name="shopBtn" Content="Shop" HorizontalAlignment="Left" Margin="505,234,0,0" VerticalAlignment="Top" Width="177" Height="49" Background="#FFD173C0" Foreground="White" FontWeight="Bold" FontSize="16" Click="shopBtn_Click" BorderBrush="#FFD173C0"/>
        <Label Content="FLOWER SHOP" HorizontalAlignment="Left" Margin="312,116,0,0" VerticalAlignment="Top" IsEnabled="False" Height="70" Width="301" FontWeight="Bold" FontFamily="Segoe Print" FontSize="36"/>
    </Grid>
</Window>
